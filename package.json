{
	"name": "ts-mugen",
	"private": true,
	"workspaces": [
		"packages/*",
		"example/*"
	],
	"license": "MIT",
	"homepage": "https://github.com/FiredRice/ts-mugen",
	"repository": {
		"type": "git",
		"url": "https://github.com/FiredRice/ts-mugen"
	},
	"scripts": {
		"link": "lerna link",
